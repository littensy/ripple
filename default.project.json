{
	"name": "ripple-test",
	"globIgnorePaths": [
		"**/default.project.json",
		"node_modules/ripple",
		"node_modules/react-ripple",
		"node_modules/vide-ripple"
	],
	"emitLegacyScripts": false,
	"tree": {
		"$className": "DataModel",
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"packages": {
				"$className": "Folder",
				"vide": {
					"$path": "node_modules/@rbxts/vide"
				},
				"ripple": {
					"$className": "Folder",
					"src": { "$path": "packages/ripple/src" }
				},
				"react-ripple": {
					"$className": "Folder",
					"src": { "$path": "packages/react-ripple/src" },
					"React": { "$path": "packages/react-ripple/React.luau" },
					"Ripple": { "$path": "packages/react-ripple/Ripple.luau" }
				},
				"vide-ripple": {
					"$className": "Folder",
					"src": { "$path": "packages/vide-ripple/src" },
					"Vide": { "$path": "packages/vide-ripple/Vide.luau" },
					"Ripple": { "$path": "packages/vide-ripple/Ripple.luau" }
				}
			},
			"@rbxts-js": {
				"$path": "node_modules/@rbxts-js"
			},
			"test": {
				"$path": "test"
			},
			"storybook": {
				"$path": "storybook"
			}
		}
	}
}

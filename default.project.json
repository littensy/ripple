{
	"name": "ripple-test",
	"globIgnorePaths": ["**/default.project.json"],
	"emitLegacyScripts": false,
	"tree": {
		"$className": "DataModel",
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"packages": {
				"$className": "Folder",
				"ripple": {
					"$className": "Folder",
					"src": { "$path": "packages/ripple" }
				}
			},
			"test": {
				"$path": "test"
			},
			"storybook": {
				"$path": "storybook"
			},
			"node_modules": {
				"$path": "node_modules/@rbxts-js"
			}
		}
	}
}
